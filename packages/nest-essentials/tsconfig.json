{
  "extends": "dev-config/ts-nest.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": "./",
    "composite": true,
    "paths": {
      "@config/*": ["src/config/*"],
      "@config": ["src/config"],
      "@logger/*": ["src/logger/*"],
      "@logger": ["src/logger"]
    }
  }
}
